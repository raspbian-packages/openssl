From 56d5a4bfcaf37fa420aef2bb881aa55e61cf5f2f Mon Sep 17 00:00:00 2001
From: Andy Polyakov <appro@openssl.org>
Date: Wed, 21 Mar 2018 23:48:10 +0100
Subject: [PATCH] pariscid.pl: fix nasty typo in CRYPTO_memcmp.

Comparison was effectively reduced to least significant bits.

CVE-2018-0733

Reviewed-by: Matt Caswell <matt@openssl.org>
---
 crypto/pariscid.pl | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/crypto/pariscid.pl b/crypto/pariscid.pl
index f82e27ac4c34..3e26f8eccfdc 100644
--- a/crypto/pariscid.pl
+++ b/crypto/pariscid.pl
@@ -160,7 +160,7 @@ L\$oop_cmp
 	or		%r29,$rv,$rv
 
 	sub		%r0,$rv,%r29
-	extru		%r29,31,1,$rv
+	extru		%r29,0,1,$rv
 L\$no_data
 	bv		($rp)
 	.EXIT
-- 
2.16.3

